{
  "name": "techleet-app",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android --device",
    "android:build": "eas build --platform android --profile preview --local",
    "android:prod": "eas build --platform android --profile production --local",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "ios": "expo run:ios --device",
    "ios:build": "eas build --platform ios --profile preview --local",
    "ios:prod": "eas build --platform ios --profile production",
    "clean": "expo prebuild --clean",
    "lint": "expo lint",
    "prebuild": "expo prebuild",
    "prepare": "husky",
    "start": "expo start",
    "submit": "eas submit",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "expo": "~54.0.18",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.12",
    "expo-haptics": "~15.0.7",
